  "steps":[
    {
      "id": 1,
      "title": "Cleaning",
      "description": "Moisten your face with warm water. Apply a small amount of cleanser to your hands and gently massage in circular motions, focusing on the T-zone and around the nose. Rinse well and pat dry with a clean towel.",
      "duration": "30-60 seconds",
      "proTips": [
        "Avoid hot water.",
        "If you use makeup or sunscreen, double cleanse."
      ]
    },
    {
      "id": 2,
      "title": "Hydration",
      "description": "Apply an appropriate amount of Moisturizer to your face and neck. Massage with upward movements until completely absorbed.",
      "duration": "30 seconds",
      "proTips": [
        "Don't forget the neck and neckline.",
        "If your skin feels heavy, use a lighter cream."
      ]
    },
    {
      "id": 3,
      "title": "Treatment",
      "description": "Apply the appropriate treatment according to your skin's needs. Use a small amount and gently pat onto face until absorbed.",
      "duration": "1 minute",
      "proTips": [
        "2-3 drops are enough.",
        "VWait 1-2 minutes for better absorption."
      ]
    },
    {
      "id": 4,
      "title": "Tonic",
      "description": "Apply a few drops of toner to your hands or on a cotton pad. Pat lightly on your face without rubbing to balance the pH and prepare the skin for the following products.",
      "duration": "10-15 seconds",
      "proTips": [
        "Alcohol-free to avoid dryness.",
        "Moisturizing in dry areas and astringent in the T zone."
      ]
    },
    {
      "id": 5,
      "title": "Sunscreen",
      "description": "Take a generous amount of sunscreen and distribute it over your face and neck. Make sure you cover all exposed areas well.",
      "duration": "1 minute",
      "proTips": [
        "Reapply every 2-3 hours if you are outdoors.",
        "Use at least 2 fingers of product."
      ]
    }
    
  ]
}
